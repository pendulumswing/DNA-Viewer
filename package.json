    "jquery": "^3.4.1",
    "json-loader": "^0.5.7",
    "json-server": "^0.15.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "now": "^15.6.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "popper.js": "^1.14.7",
