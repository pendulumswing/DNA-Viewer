{
  "name": "DNA_Viewer",
  "version": "0.0.0",
  "private": true,
  "description": "Place to try out DNA scripts for Three JS",
  "main": "index.js",
  "keywords": [],
  "license": "MIT",
  "author": "",
  "repository": {
    "type": "git",
