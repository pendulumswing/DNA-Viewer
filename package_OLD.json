    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.4.3",
    "babel-loader": "^8.0.0",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^2.0.1",
    "extract-text-webpack-plugin": "^v4.0.0-beta.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "4.0.0-alpha.2",
    "json-loader": "^0.5.7",
    "json-server": "~0.12.1",
    "less": "2.5.1",
    "less-loader": "^4.1.0",
    "node-sass": "^4.1.1",
    "now": "^15.0.0",
    "postcss-loader": "^3.0.0",
    "progress-bar-webpack-plugin": "^1.11.0",
    "rimraf": "^2.6.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "three-dragcontrols": "^0.88.2",
    "url-loader": "^1.1.2",
