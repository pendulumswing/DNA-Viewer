  "description": "Place to try out Git and Three JS",
  "homepage": "https://github.com/pendulumswing/DNA-Viewer#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pendulumswing/DNA-Viewer.git"
  },
  "bugs": {
    "url": "https://github.com/pendulumswing/DNA-Viewer/issues"
  },
  "keywords": [],
  "scripts": {
    "build": "npm run build:development",
    "build:development": "npm run clean && webpack --env.webpack=production",
    "build:production": "npm run clean && webpack --env.webpack=production -p",
    "deploy": "now",
    "deploy:production": "npm run build:production && npm run deploy",
    "clean": "rimraf dist",
    "server": "json-server --watch db.json --port=1337",
    "start": "webpack-dev-server --hot --env.webpack=development",
    "stats": "npm run stats:development",
    "stats:development": "webpack --json --env=development > stats.json",
    "stats:production": "webpack --json --env=production -p > stats.json",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "dat.gui": "^0.7.6",
    "three": "^0.104.0",
    "three-orbitcontrols": "^2.102.1",
    "three-dragcontrols": "0.88.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
