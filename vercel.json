{
    "version": 2,
    "routes": [
        { "src": "/assets/(.*)", "dest": "/dist/assets/$1" },
        { "src": "/favicon(.*)", "dest": "/dist/favicon$1" },
        { "src": "/asset-manifest.json", "dest": "/dist/asset-manifest.json" },
        { "src": "/bundle(.*)", "dest": "/dist/bundle$1" },
        { "src": "/style(.*)", "dest": "/dist/style$1" },
        { "src": "/(.*)", "dest": "/dist/index.html" }
    ]
}
